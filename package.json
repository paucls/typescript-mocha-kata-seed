{
  "name": "typescript-mocha-kata-seed",
  "version": "1.4.0",
  "description": "Seed project for TDD code katas on TypeScript and mocha",
  "main": "index.js",
  "scripts": {
    "precompile": "rimraf app/**/*.js test/**/*.js",
    "compile": "tsc",
    "test": "nyc mocha"
  },
  "author": "paucls",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "~3.5.2",
    "@types/mocha": "~2.2.41",
    "@types/node": "~7.0.18",
    "chai": "~3.5.0",
    "mocha": "~3.2.0",
    "nyc": "~11.0.3",
    "rimraf": "~2.5.2",
    "ts-node": "~3.1.0",
    "typescript": "~2.2.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ]
  }
}

